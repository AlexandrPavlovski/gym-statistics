<Window x:Class="GymStatistics.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GymStatistics"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Click="Button_Click" Margin="0,10,9.6,0" Height="26" VerticalAlignment="Top" HorizontalAlignment="Right" Width="200">Загрузить данные</Button>
        <ComboBox x:Name="combosComboBox" Margin="10,146,9.6,0" Height="22" VerticalAlignment="Top"/>
        <Label Content="ID экселины:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="sheetIdInput" Height="26" Margin="113,10,214.6,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <ComboBox x:Name="dayOfWeekComboBox" Margin="113,44,9.6,0" VerticalAlignment="Top" SelectionChanged="DayOfWeekComboBox_SelectionChanged"/>
        <Label Content="День недели:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
